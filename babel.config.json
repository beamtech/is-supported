{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "ie": "11"
        }
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "remove-import-export",
          {
            "removeImport": true,
            "removeExport": true,
            "removeExportDefault": true,
            "preseveNamedDeclaration": false
          }
        ]
      ]
    }
  }
}
